{
    "parser": "babel-eslint",
    "plugins": [
        "react"
    ],
    "extends": [
        "eslint:recommended",
        "plugin:react/recommended"
    ],
    "env": {
        "browser": true,
        "node": true,
        "es6": true,
        "amd": true
    },
    "rules": {
        "no-console":"off",
        // "react/no-direct-mutation-state":"off",
        "no-debugger":"off"
    }
}