
{
    "presets": ["@babel/react","@babel/preset-env"],
    "compact": false,
    "plugins": [
        [
            "@babel/plugin-proposal-class-properties"//此依赖可以编译剪头函数，不使用此依赖使用箭头函数会报错
        ],["@babel/plugin-proposal-decorators", { "legacy": true }],
        ["@babel/plugin-transform-runtime"]
    ]
}